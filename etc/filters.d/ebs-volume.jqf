#cmd: ec2 describe-volumes
#sort: AvailabilityZone,VolumeType,State

.Volumes | .[] | {
    AvailabilityZone,
    VolumeId,
    VolumeType,
    Size,
    State,
    Encrypted,
    Attachment: ((.Attachments | .[0] | .InstanceId) // "None"),
    AutoCleanup: ((.Attachments | .[0] | .DeleteOnTermination) // "False"),
    CreateTime
}
