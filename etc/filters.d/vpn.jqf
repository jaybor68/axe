#cmd: ec2 describe-vpn-connections
#sort: VpcId,VpnGatewayId,Name

.VpnConnections | .[]? | {
    VpnGatewayId,
    VpnConnectionId,
    CustomerGatewayId,
    Name: ((.Tags[]?|select(.Key=="Name")|.Value) // "None"),
    State,
    Type,
    Routes: ([ .Routes[].DestinationCidrBlock ] | join(", ")),
}


